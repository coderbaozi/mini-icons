{
  "name": "vite-plugin-icon",
  "type": "module",
  "version": "1.0.0-beta",
  "packageManager": "pnpm@8.1.1",
  "description": "",
  "author": "Baozi",
  "license": "MIT",
  "homepage": "https://github.com/coderbaozi/vite-plugin-icon#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbaozi/ite-plugin-icon.git"
  },
  "bugs": "https://github.com/coderbaozi/ite-plugin-icon/issues",
  "keywords": [],
  "sideEffects": false,
